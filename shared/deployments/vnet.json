{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vNetname": {
            "type": "string"
        },
        "vNetaddressPrefix": {
            "type": "string"
        },
        "busen_ag_subnetName": {
            "type": "string"
        },
        "busen_ag_subnetAddressPrefix": {
            "type": "string"
        },
        "custeng_ag_subnetName": {
            "type": "string"
        },
        "custeng_ag_subnetAddressPrefix": {
            "type": "string"
        },
        "dc_ag_subnetName": {
            "type": "string"
        },
        "dc_ag_subnetAddressPrefix": {
            "type": "string"
        },
        "ops_ag_subnetName": {
            "type": "string"
        },
        "ops_ag_subnetAddressPrefix": {
            "type": "string"
        },
        "custeng_subnetName": {
            "type": "string"
        },
        "custeng_subnetAddressPrefix": {
            "type": "string"
        },
        "dc_subnetName": {
            "type": "string"
        },
        "dc_subnetAddressPrefix": {
            "type": "string"
        },
        "ops_subnetName": {
            "type": "string"
        },
        "ops_subnetAddressPrefix": {
            "type": "string"
        },
        "busen_subnetName": {
            "type": "string"
        },
        "busen_subnetAddressPrefix": {
            "type": "string"
        },
        "enableDdosProtection": {
            "type": "bool"
        }
    },
    "resources": [
        {
            "apiVersion": "2018-08-01",
            "name": "[parameters('vNetname')]",
            "type": "Microsoft.Network/virtualNetworks",
            "location": "[resourceGroup().location]",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "[parameters('vNetaddressPrefix')]"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('busen_ag_subnetName')]",
                        "properties": {
                            "addressPrefix": "[parameters('busen_ag_subnetAddressPrefix')]"
                        }
                    },
                    {
                        "name": "[parameters('busen_subnetName')]",
                        "properties": {
                            "addressPrefix": "[parameters('busen_subnetAddressPrefix')]"
                        }
                    },
                    {
                        "name": "[parameters('custeng_ag_subnetName')]",
                        "properties": {
                            "addressPrefix": "[parameters('custeng_ag_subnetAddressPrefix')]"
                        }
                    },
                    {
                        "name": "[parameters('custeng_subnetName')]",
                        "properties": {
                            "addressPrefix": "[parameters('custeng_subnetAddressPrefix')]"
                        }
                    },
                    {
                        "name": "[parameters('dc_ag_subnetName')]",
                        "properties": {
                            "addressPrefix": "[parameters('dc_ag_subnetAddressPrefix')]"
                        }
                    },
                    {
                        "name": "[parameters('dc_subnetName')]",
                        "properties": {
                            "addressPrefix": "[parameters('dc_subnetAddressPrefix')]"
                        }
                    },
                    {
                        "name": "[parameters('ops_ag_subnetName')]",
                        "properties": {
                            "addressPrefix": "[parameters('ops_ag_subnetAddressPrefix')]"
                        }
                    },
                    {
                        "name": "[parameters('ops_subnetName')]",
                        "properties": {
                            "addressPrefix": "[parameters('ops_subnetAddressPrefix')]"
                        }
                    }
                ],
                "enableDdosProtection": "[parameters('enableDdosProtection')]"
            }
        }
    ],
    "outputs": {}
}